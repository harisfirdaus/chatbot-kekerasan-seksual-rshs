{
  "version": 2,
  "builds": [
    {
      "src": "project/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist",
        "root": "project"
      }
    },
    {
      "src": "project/api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/project/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/project/$1"
    }
  ]
} 